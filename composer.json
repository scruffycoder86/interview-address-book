{
    "config":{
        "bin-dir": "bin"
    },
    "require": {
        "illuminate/routing": "^5.7",
        "illuminate/events": "^5.8",
        "illuminate/database": "^5.8",
        "illuminate/console": "^5.8",
        "vlucas/phpdotenv": "^3.3",
        "illuminate/pagination": "^5.8",
        "illuminate/validation": "^5.8",
        "filp/whoops": "^2.3",
        "illuminate/view": "^5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "behat/behat": "^3.5",
        "phpspec/phpspec": "^5.1",
        "fzaninotto/faker": "^1.8",
        "doctrine/dbal": "^2.9",
        "symfony/var-dumper": "^4.2"
    },
    "autoload":{
        "psr-4":{
            "Distinc\\": [ "src/" ]
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev":{
        "psr-4":{
            "App\\": [ "app/" ]
        }
    }
}
